{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "react/react-in-jsx-scope": 0,
        "react/jsx-filename-extension": 0,
        "jsx-a11y/control-has-associated-label": 0
    }
}
